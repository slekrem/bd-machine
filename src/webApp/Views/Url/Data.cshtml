@model bd.machine.webApp.ViewModels.Url.UrlDataViewModel
<div class="row">
	<div class="col-md-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">@Model.RawUrl</h3>
			</div>
			<div class="panel-body">
				<table class="table">
					<thead>
						<tr>
							<th>Id</th>
							<th>Zeitstempel</th>
							<th>Vorschau</th>
						</tr>
					</thead>
					<tbody>
						@foreach(var rawHtml in Model.RawHosts) {
						<tr>
							<td>
								<a href="/RawHtml/Data/@rawHtml.Id">@rawHtml.Id</a>
							</td>
							<td>@rawHtml.Timestamp</td>
							<td>@rawHtml.Preview</td>
						</tr>
						}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>